use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function wisski_bulkedit_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.wisski_bulkedit':
      return '<p>' . t('This is the WissKI module for bulk edit operations.') . '</p>';
  }
}
